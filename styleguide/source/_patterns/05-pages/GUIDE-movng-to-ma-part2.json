  "guideSections": [{
    "title": "Converting Your Out-Of-State Driver’s License",
    "id": "converting-your-out-of-state-drivers-license",
    "pageContent": [{
      "path": "@organisms/by-author/callout-time.twig",
      "data": {
        "calloutTime": {
          "text": "This usually takes approximately 2 weeks"
        }
      }
    },{
      "path": "@organisms/by-author/rich-text.twig",
      "data": {
        "rteElements": [{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph" : {
              "text": "Once you move to Massachusetts, you must <a href=\"#\">convert your out-of-state license</a> to continue driving. You’ll need to fill out a Class D, M, or D/M License and ID Card application, which you can also submit <a href=\"#\">online</a>, and take several original forms of ID (not photocopies) to a Massachusetts RMV branch office. Common documents you may need include:"
            }
          }
        },{
          "path": "@atoms/08-lists/unordered-list.twig",
          "data": {
            "unorderedList": [{ 
              "text": "Your out-of-state license. If your out-of-state license does not have a photo, you must present another form of photo ID from the <a href=\"#\">Acceptable Identification Documents List</a>." 
            },{ 
              "text": "A document proving your date of birth, such as your original birth certificate." 
            },{ 
              "text": "A document proving your signature, such as a passport or social security card." 
            },{ 
              "text": "A document proving Massachusetts residency, such as a household bill addressed to you." 
            },{ 
              "text": "Your <a href=\"#\">Social Security number (SSN)</a>, or an acceptable alternative document (known as a “<a href=\"#\">Denial Notice</a>”) and proof of acceptable visa status along with a current non-U.S. Passport." 
            }]
          }
        },{
          "path": "@molecules/callout-stats.twig",
          "data": {
            "statsCallout": {
              "pull": "right",
              "stat": "$10-140+",
              "content": "fees vary based on license type"
            }
          }
        },{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph" : {
              "text": "<a href=\"#\">Conversion fees</a> are also required. The amount varies depending on the license class."
            }
          }
        },{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph" : {
              "text": "If your out-of-state license has been expired for more than one year but less than four years, you will be required to take a written test."
            }
          }
        },{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph" : {
              "text": "If your license has been expired for more than four years, you will need to take a written and road test, as well as a vision exam."
            }
          }
        },{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph" : {
              "text": "Keep in mind that if you later move within Massachusetts, you must <a href=\"#\">report your change of address</a> to the RMV within 30 days."
            }
          }
        }]
      }
    },{
