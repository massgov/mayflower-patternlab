{% extends '@templates/two-column.twig' %}

{% block preContent %}
  {% include "@organisms/by-template/header-alert.twig" %}
  {% include "@organisms/by-template/breadcrumbs.twig" %}
  {% include "@organisms/by-template/page-header.twig" %}
  {% include "@organisms/by-author/location-banner.twig" %}
{% endblock %}
{% block pageContent %}
  {% include "@organisms/by-author/quick-actions.twig" %}
  {% include "@organisms/by-author/action-details.twig" %}
{% endblock %}
{% block sidebar %}
  {% set quickActions = sidebar.quickActions %}
  {% include "@organisms/by-author/quick-actions.twig" %}
  {% set contactList = sidebar.contactList %}
  {% include "@organisms/by-author/contact-list.twig" %}
  {% if sidebar.eventListing %}
    {% set eventListing = sidebar.eventListing %}
    {% include "@organisms/by-author/event-listing.twig" %}
  {% endif %}
{% endblock %}
{% block postContent %}
  {% include "@organisms/by-author/suggested-pages.twig" %}
  {% include "@organisms/by-template/breadcrumbs.twig" %}
{% endblock %}
