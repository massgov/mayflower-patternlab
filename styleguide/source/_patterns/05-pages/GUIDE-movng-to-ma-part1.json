    "title": "Changing Your Address",
    "id": "changing-your-address",
    "pageContent": [{
      "path": "@organisms/by-author/rich-text.twig",
      "data": {
        "rteElements": [{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph" : {
              "text": "You may need to <a href=\"#\">file a change of address with federal agencies</a> depending on your needs. Newcomers can file a <a href=\"http://www.mass.gov\">change of address form online</a> through the <a href=\"http://www.mass.gov\">U.S. Postal Service (USPS)</a> to have mail forwarded to their new home."
            }
          }
        },{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph" : {
              "text": "Once Massachusetts residency has been established, new residents are required to have a <a href=\"#\">Massachusetts driver’s license</a> to operate a vehicle. There is no grace period."
            }
          }
        },{
          "path": "@atoms/11-text/paragraph.twig",
          "data": {
            "paragraph" : {
              "text": "If you do not drive, a <a href=\"#\">Massachusetts ID</a> can serve as proof of residency and identity."
            }
          }
        }]
      }
    },{
      "path": "@organisms/by-author/form-download.twig",
