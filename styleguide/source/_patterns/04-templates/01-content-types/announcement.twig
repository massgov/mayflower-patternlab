{% include "@organisms/by-template/emergency-alerts.twig" %}
{% include "@organisms/by-template/header.twig" %}
<main id="main-content" class="ma__announcement" tabindex="-1"{% if schemaPageType %} typeof="{{schemaPageType}}"{% endif %}>
  <div class="pre-content">
    {% block preContent %}
      {% include "@organisms/by-template/page-header.twig" %}
    {% endblock %}
  </div>
  <div class="main-content main-content--two">
    <div class="page-content ma__announcement__page-content">
      {% if pageContent.pressStatus %}
        {% set pressStatus = pageContent.pressStatus %}
        {% include "@molecules/press-status.twig" %}
      {% endif %}
      {% if sidebar.contactList %}
        {% set contactList = sidebar.contactList %}
        {% include "@organisms/by-author/contact-list.twig" %}
      {% endif %}
      {% block pageContent %}
        {% include "@base/placeholder.twig" with {'placeholder':{'text':'Page Content'}} %}
      {% endblock %}
      {% if pageContent.contactList %}
        {% set contactList = pageContent.contactList %}
        {% include "@organisms/by-author/contact-list.twig" %}
      {% endif %}
    </div>
    <aside class="sidebar ma__announcement__sidebar">
      {% if sidebar.contactList %}
        {% set contactList = sidebar.contactList %}
        {% include "@organisms/by-author/contact-list.twig" %}
      {% endif %}
      {% if sidebar.linkList %}
        {% set linkList = sidebar.linkList %}
        {% include "@organisms/by-author/link-list.twig" %}
      {% endif %}
      {% block sidebar %}
        {% include "@base/placeholder.twig" with {'placeholder':{'text':'sidebar'}} %}
      {% endblock %}
    </aside>
  </div>
  <div class="post-content">
    {% block postContent %}
      {% include "@base/placeholder.twig" with {'placeholder':{'text':'Post Content'}} %}
    {% endblock %}
  </div>
  {% block schemaContent %}{% endblock %}
</main>
{% include "@organisms/by-template/footer.twig" %}

